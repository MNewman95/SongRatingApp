
@model IEnumerable<Songs>

<h2>Songs</h2>
<table class="table">
    <tr>
        <th>Song ID</th>
        <th>Name</th>
        <th>Artist</th>
        <th>Playlist</th>
        <th>Rating</th>
    </tr>

    @foreach (var song in Model)
    {
        <tr>
           <td><a href=/Song/InsertRating/@song.songID>@song.songID</a></td>
            <td>@song.name</td>
            <td>@song.artist</td>
            <td>@song.playlistID</td>
            <td>@song.rating</td>
        </tr>
      
    }
</table>

