
@model IEnumerable<Songs>

<h2>Products</h2>
<table class="table">
    <tr>
        <th>Name</th>
        <th>Artist</th>
        <th>Playlist</th>
        <th>Rating</th>
    </tr>

    @foreach (var song in Model)
    {
        <tr>
           
            <td>@song.name</td>
            <td>@song.artist</td>
            <td>@song.playlistID</td>
            <td>@song.rating</td>
        </tr>
        <!--<a href=/Product/ViewProduct/product.ProductID>product.ProductID</a>-->
    }
</table>

