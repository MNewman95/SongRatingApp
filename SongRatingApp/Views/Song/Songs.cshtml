
@model IEnumerable<Songs>

<h2>Songs</h2>
<table class="table">
    <tr>
        <th>Rate this song</th>
        <th>Rating</th>
        <th>Name</th>
        <th>Artist</th>
        <th>Playlist</th>
        <th>Song ID</th>
    </tr>

    @foreach (var song in Model)
    {
<tr>
    <td><a href=/Song/InsertRating/@song.songID>Rate me</a></td>
    <td>@song.AverageRating </td>
    <td>@song.name</td>
    <td>@song.artist</td>
    <td>@song.PlaylistName</td>
    <td>@song.songID</td>
</tr>
        <!--<a href=/Product/ViewProduct/product.ProductID>product.ProductID</a>-->
    }
</table>

